<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
	<Nullable>enable</Nullable> <!-- Enable Nullable in Project -->
	<LangVersion>latest</LangVersion> <!-- Setting LangVersion to latest -->
	<EnableNETAnalyzers>true</EnableNETAnalyzers> <!-- Turn on Code Analysis -->
	<TreatWarningsAsErrors>true</TreatWarningsAsErrors> <!-- Treat Warnings As Errors -->
	<CodeAnalysisTreatWarningsAsErrors>true</CodeAnalysisTreatWarningsAsErrors> <!-- Turn on CATWAE -->
	<EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild> <!-- Turn on EnforceCodeStyleInBuild -->
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Interface\Interface.csproj" />
  </ItemGroup>

</Project>
